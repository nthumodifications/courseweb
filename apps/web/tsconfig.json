{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "incremental": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"],
      "@courseweb/ui": ["../../packages/ui/src"],
      "@courseweb/shared": ["../../packages/shared/src"],
      "@courseweb/database": ["../../packages/database/src"]
    },
    "types": ["vite/client"]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/vite-env.d.ts"],
  "exclude": ["node_modules"]
}
