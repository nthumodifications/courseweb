{
  "name": "@courseweb/mobile",
  "version": "1.0.0",
  "description": "CourseWeb mobile application built with Capacitor",
  "main": "index.js",
  "scripts": {
    "build": "npm run sync && cap build",
    "dev": "cap run",
    "sync": "cap sync",
    "sync:ios": "cap sync ios && cap open ios",
    "sync:android": "cap sync android && cap open android",
    "build:ios": "cap build ios",
    "build:android": "cap build android",
    "open:ios": "cap open ios",
    "open:android": "cap open android",
    "add:ios": "cap add ios",
    "add:android": "cap add android",
    "copy": "cap copy",
    "update": "cap update",
    "doctor": "cap doctor",
    "clean:ios": "rm -rf ios/App/build ios/App/DerivedData",
    "clean:android": "cd android && ./gradlew clean",
    "clean": "npm run clean:ios && npm run clean:android",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "courseweb",
    "mobile",
    "capacitor",
    "ionic",
    "nthumods"
  ],
  "author": "CourseWeb Team",
  "license": "MIT",
  "dependencies": {
    "@capacitor-community/media": "6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "capacitor-set-version": "^2.2.0"
  },
  "devDependencies": {
    "@courseweb/eslint-config": "*",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.0"
  },
  "eslintConfig": {
    "extends": [
      "@courseweb/eslint-config"
    ]
  }
}
