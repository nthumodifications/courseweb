{
  "name": "courseweb",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.0.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*",
    "tools/*"
  ],
  "contributors": [
    "Chew Tzi Hwee <chewtzihwee@gmail.com>(https://imjustchew.com)"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean",
    "dev:web": "turbo run dev --filter=@courseweb/web",
    "build:web": "turbo run build --filter=@courseweb/web",
    "build:api": "turbo run build --filter=@courseweb/api",
    "build:secure-api": "turbo run build --filter=@courseweb/secure-api",
    "build:apis": "turbo run build --filter=@courseweb/api --filter=@courseweb/secure-api",
    "build:api-types": "turbo run build --filter=@courseweb/api-types",
    "dev:api": "turbo run dev --filter=api",
    "dev:secure-api": "turbo run dev --filter=secure-api",
    "runner:update-courses": "node --import=tsimp/import tools/data-sync/src/update-courses.ts",
    "runner:sync-courses": "node --import=tsimp/import tools/data-sync/src/sync-courses.ts",
    "dict": "node --import=tsimp/import tools/dict-manager/src/dict.ts",
    "sync:once": "bun run --workspace=@courseweb/data-sync sync:once",
    "sync:scheduled": "bun run --workspace=@courseweb/data-sync sync:scheduled",
    "dict:create": "bun run --workspace=@courseweb/dict-manager create",
    "dict:remove": "bun run --workspace=@courseweb/dict-manager remove",
    "dict:move": "bun run --workspace=@courseweb/dict-manager move",
    "build:update-imports": "bun run --workspace=@courseweb/build-scripts update:web-imports",
    "docker:build": "docker build -f apps/web/Dockerfile -t nthumods-web .",
    "docker:run": "docker run -p 3000:3000 nthumods-web",
    "gentype": "bunx supabase gen types typescript --project-id \"cmzdlrqfpuktcczvsobs\" --schema public > packages/shared/src/types/supabase.ts",
    "prepare": "husky"
  },
